<div class="container">
<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>
  <div class="card-header_new_event">
    <div class="form-inputs" >
          <div class="text-center">
          <div class="card-upload">
          <p><%= f.input :picture, as: :attachinary %></p>
          </div>
          <div class="card-event">
          <p><%= f.input :name, label: "Name of your event" %></p>
          </div>
          <div class="card-price">
          <p> <%= f.input :price %></p>
          </div>
           <div class="card-date">
          <p><%= f.input :date %></p>
          </div>
          <div class="card-place">
          <p><%= f.input :place %></p>
          </div>
          <div class="card-categoriey">
          <p><%= f.input :category_id, collection: Category.all  %></p>
          </div>
          <div class="card-description">
          <p><%= f.input :description %></p>
          </div>
          <div class="card-address">
          <p><%= f.input :address %></p>
          </div>
          <div class="card-zip">
          <p> <%= f.input :zip_code %></p>
          </div>
          <div class="card-city">
          <p><%= f.input :city %></p>
          </div>
          <div class="card-country">
          <p><%= f.input :country, priority: [ 'FR' ] %></p>
          </div>
          <%= f.submit %>
        </div>
      </div>
   </div>
 <% end %>
</div>

<%= content_for(:after_js) do %>

<script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyDANjx3bosEtIyzJaoWs50Wnt6nt_1rmxU"></script>
<% end %>
